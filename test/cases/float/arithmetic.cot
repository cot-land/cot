// EXPECT: exit_code=0
fn main() int {
    let a: f64 = 10.0;
    let b: f64 = 3.0;
    // Addition
    let sum = a + b;
    if sum < 12.9 { return 1; }
    if sum > 13.1 { return 2; }
    // Subtraction
    let diff = a - b;
    if diff < 6.9 { return 3; }
    if diff > 7.1 { return 4; }
    // Multiplication
    let prod = a * b;
    if prod < 29.9 { return 5; }
    if prod > 30.1 { return 6; }
    // Division
    let quot = a / b;
    if quot < 3.3 { return 7; }
    if quot > 3.4 { return 8; }
    return 0;
}
