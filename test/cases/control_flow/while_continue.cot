// EXPECT: exit_code=20
// Sum only even numbers from 0-9: 0+2+4+6+8 = 20
fn main() int {
    let i: int = 0;
    let sum: int = 0;
    while (i < 10) {
        if (i % 2 == 1) {
            i = i + 1;
            continue;
        }
        sum = sum + i;
        i = i + 1;
    }
    return sum;
}
