enum Level {
    Low = 10,
    Medium = 50,
    High = 100
}

test "simple" {
    let x: i64 = 2
    let result = switch x {
        1 => 10,
        2 => 20,
        3 => 30,
        else => 0,
    }
    @assert_eq(result, 20)
}

test "enum switch" {
    let lvl = Level.Medium
    let val = switch lvl {
        Level.Low => 1,
        Level.Medium => 50,
        Level.High => 99,
        else => 0,
    }
    @assert_eq(val, 50)
}
